<h2>Horticulturists:</h2>
            <%if @count.nil?%>
              <h3>There are no horticulturists presently </h3>
            <%else%>

<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <div class="table-responsive">
        <table class="table table-bordered table-hover">
          <thead>
            <tr>
              <th>Name</th>
              <th>Profile</th>
            </tr>
          </thead>
          <tbody>

 <% @horticulturists.each do |h| %>
      <tr>
        <td><%= h.name %></td>
        <td><%= link_to 'View', dashboard_path(h.id) %></td>
      </tr>
    <% end %>
         </tbody>
        </table>
      </div><!--end of .table-responsive-->
    </div>
  </div>
</div>
  <%end%>


