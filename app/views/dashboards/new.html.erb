<br/>
<h2>Enter Profile Information : </h2>
<br/>
<%= form_for(:dashboard,url: dashboards_create_path) do |f|%>
 <%= render 'shared/error_messages', object: @dashboard %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <br/>
  <div class="field">
    <%= f.label :qualification %><br>
    <%= f.text_area :qualification %>
  </div>
  <br/>
  <div class="field">
    <%= f.label :contact %><br>
    <%= f.text_area :contact %>
  </div>
  <br/>
  <div class="actions">
    <%= f.submit %>
  </div>

<%end%>
